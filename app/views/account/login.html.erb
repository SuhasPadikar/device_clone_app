<h2>Login</h2>
<%= flash[:notice] %>
<h4><%= form_with url:"/account/login", method: :post do |f| %>


<div class="field">
  <%= f.label :email%><br/>
  <%= f.text_field :email%>
</div>


<div class="field">
  <%= f.label :password%><br/>
  <%= f.password_field :password%>
</div>

<div class="actions">
  <%= f.submit "Login"%>
</div>
</h4>
<% end %>


<%= link_to 'Forgot password', password_forgot_password_path %>
